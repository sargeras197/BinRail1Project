{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build demo (demo.c + etslib.c)",
      "type": "shell",
      "command": "gcc",
      "args": [
        "-O2",
        "-ffunction-sections",
        "-fdata-sections",
        "-Wl,--gc-sections",
        "demo.c",
        "etslib.c",
        "-o",
        "demo.exe"
      ],
      "group": { "kind": "build", "isDefault": true },
      "problemMatcher": ["$gcc"]
    }
  ]
}
